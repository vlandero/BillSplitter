{
  "expo": {
    "name": "BillSplitter",
    "slug": "BillSplitter",
    "owner": "vlanderoorg",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A simple bill splitter which adds up every payment made in a group of friends and shows the amount of money they have to pay to each other.\nOnly works on iOS",
    "githubUrl": "https://github.com/vlandero/BillSplitter-React-Native"
  }
}
